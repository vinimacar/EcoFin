    {
      "protocol": "web+ecofin",
      "url": "/?action=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/import",
      "accept": {
        "application/json": [".json"],
        "text/csv": [".csv"],
        "application/vnd.ms-excel": [".xls", ".xlsx"]
      },
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "72x72",
          "type": "image/svg+xml"
